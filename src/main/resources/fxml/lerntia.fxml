<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="mainWindow" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.tuwien.sepm.assignment.groupphase.lerntia.ui.LerntiaMainController">
    <VBox fx:id="mainWindowLeft" prefHeight="768.0" prefWidth="445.0" styleClass="main-left" stylesheets="@/css/background-division.css">
        <VBox prefHeight="44.0" prefWidth="453.0">
            <fx:include fx:id="menuBar" source="/fxml/elements/menuBar.fxml" />
        </VBox>
        <HBox prefHeight="134.0">
            <VBox prefHeight="134.0" prefWidth="323.0">
            <children>
               <Pane prefHeight="134.0" prefWidth="147.0" />
            </children></VBox>
            <fx:include fx:id="audioButton" source="/fxml/elements/audioButton.fxml" />
        </HBox>
        <HBox prefHeight="17.0" prefWidth="474.0" />
        <VBox prefHeight="588.0" prefWidth="473.0">
            <ImageView fx:id="mainImage" fitHeight="300.0" fitWidth="474.0" pickOnBounds="true" preserveRatio="true" />
            <HBox>
                <VBox prefHeight="134.0" prefWidth="323.0" />
            </HBox>
        </VBox>
    </VBox>
    <VBox fx:id="mainWindowRight" prefHeight="768" styleClass="main-right" stylesheets="@/css/background-division.css">
        <HBox prefHeight="30.0" />
        <HBox prefHeight="286.0" styleClass="question-box">
            <fx:include fx:id="qLabel" source="/fxml/elements/questionLabel.fxml" />
        </HBox>
        <HBox layoutX="10.0" layoutY="10.0" prefHeight="250.0" prefWidth="2919.0" styleClass="answer-box" stylesheets="@/css/background-division.css">
            <fx:include fx:id="answer1" source="/fxml/elements/answerCheckBox.fxml" />
        </HBox>
        <HBox layoutX="10.0" layoutY="208.0" prefHeight="250.0" prefWidth="1024.0" styleClass="answer-box" stylesheets="@/css/background-division.css">
            <fx:include fx:id="answer2" source="/fxml/elements/answerCheckBox.fxml" />
        </HBox>
        <HBox layoutX="10.0" layoutY="345.0" prefHeight="250.0" prefWidth="2756.0" styleClass="answer-box" stylesheets="@/css/background-division.css">
            <fx:include fx:id="answer3" source="/fxml/elements/answerCheckBox.fxml" />
        </HBox>
        <HBox layoutX="10.0" layoutY="482.0" prefHeight="250.0" prefWidth="1024.0" styleClass="answer-box" stylesheets="@/css/background-division.css">
            <fx:include fx:id="answer4" source="/fxml/elements/answerCheckBox.fxml" />
        </HBox>
        <HBox layoutX="10.0" layoutY="619.0" prefHeight="250.0" prefWidth="1024.0" styleClass="answer-box" stylesheets="@/css/background-division.css">
            <fx:include fx:id="answer5" source="/fxml/elements/answerCheckBox.fxml" />
        </HBox>
        <HBox layoutX="10.0" layoutY="757.0" prefHeight="105.0" prefWidth="1024.0" styleClass="button-box" stylesheets="@/css/background-division.css">
            <ButtonBar fx:id="buttonBar" prefHeight="98.0">
                <buttons>
                    <fx:include fx:id="previousQuestionButton" source="/fxml/elements/previousQuestionButton.fxml" />
                    <fx:include fx:id="nextQuestionButton" source="/fxml/elements/nextQuestionButton.fxml" />
                    <VBox prefHeight="95.0" prefWidth="40.0">
                  <children>
                        <fx:include fx:id="learnalgorithmbutton" source="/fxml/elements/learnAlgorithmButton.fxml" />
                  </children></VBox>
                    <fx:include fx:id="checkAnswerButton" source="/fxml/elements/checkAnswerButton.fxml" />
                    <fx:include fx:id="handInButton" source="/fxml/elements/handInButton.fxml" />
                </buttons>
            </ButtonBar>
        </HBox>
    </VBox>
</HBox>
